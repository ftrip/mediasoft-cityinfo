{
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "no-console": 0,
    "comma-dangle": "off",
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0
  },
  "globals": {
    "document": true,
    "window": true
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "extends": ["airbnb-base", "plugin:react/recommended"]
}